{
    "name": "dcat-admin/extension-grid-sortable",
    "description": "Grid Sortable Extension for Dcat Admin",
    "type": "library",
    "keywords": [
        "dcat-admin",
        "extension"
    ],
    "homepage": "https://github.com/elysiumrealms/extension-grid-sortable",
    "license": "MIT",
    "authors": [
        {
            "name": "deflinhec",
            "email": "deflinhec@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "dcat/laravel-admin": "~2.0",
        "spatie/eloquent-sortable": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dcat\\Admin\\GridSortable\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "Dcat\\Admin\\GridSortable\\GridSortableServiceProvider",
        "laravel": {
            "providers": [
                "Dcat\\Admin\\GridSortable\\GridSortableServiceProvider"
            ]
        }
    }
}